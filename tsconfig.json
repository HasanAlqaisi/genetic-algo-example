{
  "include": ["src"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "types": ["node"],
    "resolveJsonModule": true,
    "allowJs": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
